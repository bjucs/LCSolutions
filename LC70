class Solution:
    def climbStairs(self, n: int) -> int:
        #Climbing Stairs using Stacks (fibonacci)
        
        if n <= 3:
            return n 
        
        stack = []
        stack.append(2)
        stack.append(3)
        
        for x in range(3, n): 
            first, second = stack.pop(), stack.pop() 
            stackSum = first + second 
            stack.append(first)
            stack.append(stackSum)
        
        return stackSum
            
            
        
        
